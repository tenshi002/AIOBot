{% block flashMessage %}
	<div class="alert alert-{{ type }} alert-dismissible fade in" role="alert">
		<button type="button" class="close" data-dismiss="alert" aria-label="Fermer">
			<span aria-hidden="true">&times;</span></button>
		<strong>{{ title }}</strong>
		<p>{{ message }}</p>
	</div>
{% endblock %}